local function collectItem(D)
local E=m.Character
if not E or not E:FindFirstChild("HumanoidRootPart")then
return false
end

local F=D:FindFirstChild("ItemRoot")
if F and F:FindFirstChild("ProximityPrompt")then
local G=F.ProximityPrompt
E.HumanoidRootPart.CFrame=F.CFrame
task.wait(0.5)
fireproximityprompt(G)
task.wait(1)
return true
end
return false
end

local function createESP(D,E,F,G,H)
if D:FindFirstChild("TAOWARE_Highlight")then return end
if F and D:FindFirstChild("Progress")and D.Progress.Value==100 then return end

local I
if F then
I=D:FindFirstChild("Main")
if not I then return end
elseif G then
I=D:FindFirstChild("ItemRoot")
if not I then return end
elseif H then
I=D
if not I then return end
else
I=D:FindFirstChild("HumanoidRootPart")
if not I then return end
end

local J=Instance.new("Highlight")
J.Name="TAOWARE_Highlight"
J.Adornee=D
J.FillColor=E
J.OutlineColor=E
J.FillTransparency=0.5
J.OutlineTransparency=0
J.Parent=D

local K=Instance.new("BillboardGui")
K.Name="TAOWARE_Billboard"
K.Adornee=I
K.Size=UDim2.new(0,150,0,60)
K.StudsOffset=Vector3.new(0,4,0)
K.AlwaysOnTop=true
K.Parent=D

if not F and not G and not H then
local L=a:GetPlayerFromCharacter(D)
local M=L and L.Name or"Unknown"
local N=D:FindFirstChild("Humanoid")
local O=N and string.format("%.0f",N.Health)or"N/A"
local P="Unknown"
if L and L:FindFirstChild("PlayerData")and L.PlayerData:FindFirstChild("Equipped")then
if D.Parent.Name=="Killers"and L.PlayerData.Equipped:FindFirstChild("Killer")then
P=L.PlayerData.Equipped.Killer.Value
elseif D.Parent.Name=="Survivors"and L.PlayerData.Equipped:FindFirstChild("Survivor")then
P=L.PlayerData.Equipped.Survivor.Value
end
end

local Q=Instance.new("TextLabel")
Q.Size=UDim2.new(1,0,0.33,0)
Q.Position=UDim2.new(0,0,0,0)
Q.BackgroundTransparency=1
Q.Text=M
Q.Font=Enum.Font.GothamBlack
Q.TextColor3=E
Q.TextSize=12
Q.TextStrokeTransparency=0.6
Q.TextStrokeColor3=Color3.fromRGB(0,0,0)
Q.Parent=K

local R=Instance.new("TextLabel")
R.Size=UDim2.new(1,0,0.33,0)
R.Position=UDim2.new(0,0,0.33,0)
R.BackgroundTransparency=1
R.Text="HP: "..O
R.Font=Enum.Font.GothamBlack
R.TextColor3=E
R.TextSize=12
R.TextStrokeTransparency=0.6
R.TextStrokeColor3=Color3.fromRGB(0,0,0)
R.Parent=K

local S=Instance.new("TextLabel")
S.Size=UDim2.new(1,0,0.33,0)
S.Position=UDim2.new(0,0,0.66,0)
S.BackgroundTransparency=1
S.Text=P
S.Font=Enum.Font.GothamBlack
S.TextColor3=E
S.TextSize=12
S.TextStrokeTransparency=0.6
S.TextStrokeColor3=Color3.fromRGB(0,0,0)
S.Parent=K
else
local L=Instance.new("TextLabel")
L.Size=UDim2.new(1,0,1,0)
L.BackgroundTransparency=1
L.Text=D.Name
L.Font=Enum.Font.GothamBlack
L.TextColor3=E
L.TextSize=F and 14 or 12
L.TextStrokeTransparency=0.6
L.TextStrokeColor3=Color3.fromRGB(0,0,0)
L.Parent=K
end
end

local function removeESP(D)
if D:FindFirstChild("TAOWARE_Highlight")then
D.TAOWARE_Highlight:Destroy()
end
if D:FindFirstChild("TAOWARE_Billboard")then
D.TAOWARE_Billboard:Destroy()
end
end

local function getClosestTarget(D)
local E=d:FindFirstChild("Players")
if not E then return nil end

local F
if D=="killer"then
local G=E:FindFirstChild("Killers")
if G then
F=G:GetChildren()
else
F={}
end
elseif D=="survivor"then
local G=E:FindFirstChild("Survivors")
if G then
F=G:GetChildren()
else
F={}
end
else
return nil
end

local G=nil
local H=p.aimbotFOV

for I,J in ipairs(F)do
if J==m.Character then continue end
local K=J:FindFirstChildOfClass("Humanoid")
if not K or K.Health<=0 then continue end
local L=J:FindFirstChild("HumanoidRootPart")
if not L then continue end

local M,N=n:WorldToViewportPoint(L.Position)
if not N then continue end

local O=(Vector2.new(M.X,M.Y)-Vector2.new(n.ViewportSize.X/2,n.ViewportSize.Y/2)).Magnitude
if O>p.aimbotFOV then continue end

if p.aimbotWallCheck then
local P=n.CFrame.Position
local Q=L.Position-P
local R=RaycastParams.new()
R.FilterDescendantsInstances={m.Character,J}
R.FilterType=Enum.RaycastFilterType.Blacklist
local S=d:Raycast(P,Q,R)
if S then continue end
end

if O<H then
H=O
G=J
end
end

return G
end


local function isGameInProgress()
local D=d:FindFirstChild("Map")
if not D or not D:FindFirstChild("Ingame")then
return false
end

local E=d:FindFirstChild("Players")
if E then
local F=E:FindFirstChild("Survivors")
if F and#F:GetChildren()>0 then
return true
end
end

return false
end


local function hasGenerators()
local D=d:FindFirstChild("Map")
if D and D:FindFirstChild("Ingame")and D.Ingame:FindFirstChild("Map")then
for E,F in ipairs(D.Ingame.Map:GetChildren())do
if F:IsA("Model")and F.Name:lower():find("generator")then
return true
end
end
end
return false
end


local D=false

local function runAutoGenerator()
if D then return end
D=true
task.spawn(function()
while D and p.autoGenerator and hasGenerators()and isGameInProgress()do
pcall(function()
local E=d:FindFirstChild("Map")
if not E then return end
local F=E:FindFirstChild("Ingame")
if not F then return end
local G=F:FindFirstChild("Map")
if not G then return end

for H,I in ipairs(G:GetChildren())do
if I:IsA("Model")and I.Name:lower():find("generator")then
local J=I:FindFirstChild("Remotes")
if J then
local K=J:FindFirstChild("RE")
local L=J:FindFirstChild("RF")
if K and L then
pcall(function()K:FireServer()end)
pcall(function()L:InvokeServer()end)
end
end
task.wait(0.1)
end
end
end)
task.wait(p.generatorWaitTime)
end
D=false
end)
end

local function restartAutoGenerator()
D=false
task.wait(0.1)
if p.autoGenerator then
runAutoGenerator()
end
end


