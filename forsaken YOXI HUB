local ap,aq,ar
do
local as,at=pcall(function()
return require(c.Systems.Character.Game.Sprinting)
end)
if as and at then
ap=at.DefaultConfig.SprintSpeed or 26
aq=at.DefaultConfig.StaminaLoss or 10
ar=at.DefaultConfig.MaxStamina or 100
else
ap=26
aq=10
ar=100
end
end

local function updateAllESP()
local as=d:FindFirstChild("Map")
if not as or not as:FindFirstChild("Ingame")then return end
local at=as.Ingame

if p.pizzaDeliveryEsp then
for au,av in ipairs(at:GetChildren())do
if table.find(C,av.Name)and av:FindFirstChild("HumanoidRootPart")then
if not av:FindFirstChild("TAOWARE_Highlight")then
createESP(av,Color3.fromRGB(255,0,0),false,false,false)
end
end
end
end

if p.zombieEsp then
for au,av in ipairs(at:GetChildren())do
if av.Name=="1x1x1x1Zombie"and av:FindFirstChild("HumanoidRootPart")then
if not av:FindFirstChild("TAOWARE_Highlight")then
createESP(av,Color3.fromRGB(255,0,0),false,false,false)
end
end
end
end

if p.pizzaEsp then
local au=at:FindFirstChild("Pizza")
if au and not au:FindFirstChild("TAOWARE_Highlight")then
createESP(au,Color3.fromRGB(255,165,0),false,false,true)
end
end

if not isGameInProgress()then
return
end

local au=d:FindFirstChild("Players")
if au then
local av=au:FindFirstChild("Killers")
if av then
for aw,ax in ipairs(av:GetChildren())do
removeESP(ax)
if p.killerESP and ax:FindFirstChild("HumanoidRootPart")then
createESP(ax,Color3.fromRGB(255,0,0),false,false,false)
end
end
end

local aw=au:FindFirstChild("Survivors")
if aw then
for ax,ay in ipairs(aw:GetChildren())do
removeESP(ay)
if p.playerESP and ay:FindFirstChild("HumanoidRootPart")then
createESP(ay,Color3.fromRGB(0,255,0),false,false,false)
end
end
end
end

if at:FindFirstChild("Map")then
for av,aw in ipairs(at.Map:GetChildren())do
if aw:IsA("Model")and aw.Name:lower():find("generator")then
removeESP(aw)
local ax=aw:FindFirstChild("Progress")
if p.generatorESP and ax and ax.Value<100 then
createESP(aw,Color3.fromRGB(255,0,255),true,false,false)
end
end
end
end

local av={"1x1x1x1Zombie"}
for aw,ax in ipairs(C)do
table.insert(av,ax)
end
for aw,ax in ipairs(at:GetDescendants())do
if ax:IsA("Model")and ax:FindFirstChild("ItemRoot")and not table.find(av,ax.Name)then
removeESP(ax)
if p.itemESP then
createESP(ax,Color3.fromRGB(255,255,0),false,true,false)
end
end
end
end


task.spawn(function()
while true do
if p.autoCollectItems and isGameInProgress()then
local as=d:FindFirstChild("Map")
if not as or not as:FindFirstChild("Ingame")then
task.wait(0.1)
continue
end
for at,au in ipairs(as.Ingame:GetDescendants())do
if not p.autoCollectItems then break end
if au.Name=="ItemRoot"and au:IsA("BasePart")then
local av=au:FindFirstChild("ProximityPrompt")
if av then
collectItem(au.Parent)
task.wait(0.5)
end
end
end
task.wait(0.1)
else
task.wait(0.1)
end
end
end)


task.spawn(function()
local as,at=pcall(function()
return require(c.Systems.Character.Game.Sprinting)
end)
if not as or not at then return end

b.RenderStepped:Connect(function()
if p.speedBoost then
at.SprintSpeed=p.sprintSpeed
end
if p.noStaminaLoss then
at.MaxStamina=999999
at.Stamina=999999
else
at.MaxStamina=ar
at.Stamina=math.clamp(at.Stamina,at.MinStamina,ar)
end
end)
end)


local function isPlasmaBeamActive()
local as=m.Character
if not as then return false end
local at=as:FindFirstChild("SpeedMultipliers")
if not at then return false end
return at:FindFirstChild("PlasmaBeam")~=nil
end

local function isShootSkillActive()
local as=m.Character
if not as then return false end
local at=as:FindFirstChild("SpeedMultipliers")
if not at then return false end
return at:FindFirstChild("ShootingGun")~=nil
end

local function isMassInfectionActive()
local as=m.Character
if not as then return false end
local at=as:FindFirstChild("SpeedMultipliers")
if not at then return false end
return at:FindFirstChild("MassInfection")~=nil
end

local function isEntanglementActive()
local as=m.Character
if not as then return false end
local at=as:FindFirstChild("SpeedMultipliers")
if not at then return false end
return at:FindFirstChild("Entanglement")~=nil
end


task.spawn(function()
local function isAnySkillActive()
return(p.aimBotPlasmaBeamSurvivor and isPlasmaBeamActive())or
(p.aimBotPlasmaBeamKiller and isPlasmaBeamActive())or
(p.aimBotShootKiller and isShootSkillActive())or
(p.aimBotMassInfection and isMassInfectionActive())or
(p.aimBotEntanglement and isEntanglementActive())
end

b.RenderStepped:Connect(function()
local as=isAnySkillActive()
if as and not z then
z=true
f.MouseBehavior=Enum.MouseBehavior.LockCenter
elseif not as and z then
z=false
f.MouseBehavior=Enum.MouseBehavior.Default
end

if not(p.aimBotPlasmaBeamSurvivor or p.aimBotPlasmaBeamKiller or p.aimBotShootKiller or
p.aimBotMassInfection or p.aimBotEntanglement)or not as then
if y then
y:Disconnect()
y=nil
end
return
end

if not y then
y=b.RenderStepped:Connect(function()
local at=nil
if isPlasmaBeamActive()then
if p.aimBotPlasmaBeamSurvivor then
at=getClosestTarget("survivor")
elseif p.aimBotPlasmaBeamKiller then
at=getClosestTarget("killer")
end
elseif isShootSkillActive()and p.aimBotShootKiller then
at=getClosestTarget("killer")
elseif isMassInfectionActive()and p.aimBotMassInfection then
at=getClosestTarget("survivor")
elseif isEntanglementActive()and p.aimBotEntanglement then
at=getClosestTarget("survivor")
end

if at and at:FindFirstChild("HumanoidRootPart")then
local au=at.HumanoidRootPart.Position
local av=CFrame.new(n.CFrame.Position,au)
if p.aimbotSmoothing then
n.CFrame=n.CFrame:Lerp(av,p.aimbotAimStrength)
else
n.CFrame=av
end

if(p.aimBotShootKiller and isShootSkillActive())or
(p.aimBotMassInfection and isMassInfectionActive())or
(p.aimBotEntanglement and isEntanglementActive())then
local aw=m.Character.HumanoidRootPart.Position
local ax=(au-aw).Unit
m.Character.HumanoidRootPart.CFrame=CFrame.new(aw,aw+ax*10)
end
end
end)
end
end)
end)


do
local as,at=pcall(function()
return require(c.Modules.StatusEffects.SurvivorExclusive.Subspaced)
end)
if as and at then
at.Applied=function()end
at.Removed=function()end
end
end


do
local as,at=pcall(function()
return require(c.Modules.StatusEffects.KillerExclusive.Glitched)
end)
if as and at then
local au=at.Applied
at.Applied=function(av)
if p.antiGlitchedEffect then
return
end
return au(av)
end
end
end


do
local as,at=pcall(function()
return require(c.Modules.StatusEffects.Blindness)
end)
if as and at then
local au=at.Applied
at.Applied=function(av)
if p.antiBlindness then
return
end
return au(av)
end
end
end


task.spawn(function()
while true do
local as=d:FindFirstChild("Players")
if as then
for at,au in ipairs(as:GetChildren())do
for av,aw in ipairs(au:GetChildren())do
if aw:IsA("Model")then
if p.antiWalkSpeedOverride then
local ax=aw:FindFirstChild("Result")
if ax then
ax:Destroy()
end
end
local ax=aw:FindFirstChild("SpeedMultipliers")
if ax then
local ay=ax:FindFirstChild("HinderedMovement")
if ay and(p.antiCorruptNature or p.antiPizzaDelivery)then
ay.Value=1.8
end
if p.antiMassInfection then
local az=ax:FindFirstChild("MassInfection")
if az then
az.Value=1.8
end
end
if p.antiEntanglement then
local az=ax:FindFirstChild("Entanglement")
if az then
az.Value=1.8
end
end
local az=ax:FindFirstChild("SlowedStatus")
if az then
az:Destroy()
end
local aA=ax:FindFirstChild("FallSlowness")
if aA and p.antiFallDebuff then
aA:Destroy()
end

if p.antiColaDebuff then
local aB=ax:FindFirstChild("DrinkingCola")
if aB then
aB.Value=1.8
end
local aC=ax:FindFirstChild("BloxyColaItem")
if aC then
aC.Value=1.8
end
end

if p.antiSlateskinDebuff then
local aB=ax:FindFirstChild("DrinkingSlateskin")
if aB then
aB.Value=1.8
end
local aC=ax:FindFirstChild("SlateskinStatus")
if aC then
aC.Value=1.8
end
end

if p.antiBurgerDebuff then
local aB=ax:FindFirstChild("EatingGhostburger")
if aB then
aB.Value=1.8
end
end

if p.antiSlashDebuff then
local aB=ax:FindFirstChild("StunningKiller")
if aB then
aB.Value=1.8
end
end

if p.antiChickenDebuff then
local aB=ax:FindFirstChild("EatFriedChicken")
if aB then
aB.Value=1.8
end
end

if p.antiBlockDebuff then
local aB=ax:FindFirstChild("GuestBlocking")
if aB then
aB.Value=1.8
end
end

if p.bypassChargeSpeed then
local aB=ax:FindFirstChild("Guest1337Charge")
if aB then
aB.Value=3.4
end
end

if p.bypassPunchSpeed then
local aB=ax:FindFirstChild("PunchAbility")
if aB then
aB.Value=1.8
end
end

if p.antiTaphTripwire then
local aB=ax:FindFirstChild("TaphTripwire")
if aB then
aB.Value=1.8
end
end

if p.antiSubspaceTripmine then
local aB=ax:FindFirstChild("SubspaceTripmine")
if aB then
aB.Value=1.8
end
end

if p.antiPlasmaBeam then
local aB=ax:FindFirstChild("PlasmaBeam")
if aB then
aB.Value=1.8
end
end

if p.antiSpawnProtection then
local aB=ax:FindFirstChild("SpawnProtection")
if aB then
aB.Value=1.8
end
end

if p.antiTwoTimeCrouching then
local aB=ax:FindFirstChild("TwoTimeCrouching")
if aB then
aB.Value=1.8
end
end

if p.antiTwoTimeStab then
local aB=ax:FindFirstChild("TwoTimeStab")
if aB then
aB.Value=1.8
end
end

if p.antiCoolGui then
local aB=ax:FindFirstChild("c00lgui")
if aB then
aB.Value=1.8
end
end

if p.antiBehead then
local aB=ax:FindFirstChild("BeheadAbility")
if aB then
aB.Value=1.8
end
end

if p.antiEnraged then
local aB=ax:FindFirstChild("ENRAGED")
if aB then
aB.Value=2.7
end
end

if p.antiShootDebuff then
local aB=ax:FindFirstChild("ShootingGun")
if aB then
aB.Value=1.8
end
end
end

local ay=aw:FindFirstChild("FOVMultipliers")
if ay then
local az=ay:FindFirstChild("SlowedStatus")
if az and(p.autoRemovePopups or p.antiSlowAfterChargeEnd or p.antiSpawnProtection)then
az:Destroy()
end

if p.antiSlateskinDebuff then
local aA=ay:FindFirstChild("SlateskinStatus")
if aA then
aA:Destroy()
end
end

if p.antiAutoFovCharge then
local aA=ay:FindFirstChild("Guest1337Charge")
if aA then
aA:Destroy()
end
end
end
end
end
end

if p.autoRemovePopups then
for at,au in ipairs(a:GetPlayers())do
local av=au:FindFirstChild("PlayerGui")
if av then
local aw=av:FindFirstChild("TemporaryUI")
if aw then
local ax=aw:FindFirstChild("1x1x1x1Popup")
if ax then
ax:Destroy()
end
end
end
end
end

if p.antiMedkitDebuff then
local at,au=pcall(function()
return require(c.Assets.Items.Medkit.Config)
end)
if at and au then
au.HealTime=0
au.HealSpeedMultiplier=1
end
end

task.wait(0.05)
else
task.wait(0.1)
end
end
end)


if not _G.FullBrightExecuted then
_G.FullBrightEnabled=false
_G.NormalLightingSettings={
Brightness=game:GetService("Lighting").Brightness,
ClockTime=game:GetService("Lighting").ClockTime,
FogEnd=game:GetService("Lighting").FogEnd,
GlobalShadows=game:GetService("Lighting").GlobalShadows,
Ambient=game:GetService("Lighting").Ambient
}

local function removeEffects()
local as=game:GetService("Lighting")
for at,au in ipairs({"DepthOfField","ColorCorrection","Blur","Atmosphere"})do
local av=as:FindFirstChild(au)
if av then
av:Destroy()
end
end
end

local as=game:GetService("Lighting")
as.Changed:Connect(function(at)
if _G.FullBrightEnabled then
if at=="Brightness"then
as.Brightness=1
elseif at=="ClockTime"then
as.ClockTime=12
elseif at=="FogEnd"then
as.FogEnd=786543
elseif at=="GlobalShadows"then
as.GlobalShadows=false
elseif at=="Ambient"then
as.Ambient=Color3.fromRGB(178,178,178)
end
removeEffects()
else
if at=="Brightness"then
_G.NormalLightingSettings.Brightness=as.Brightness
elseif at=="ClockTime"then
_G.NormalLightingSettings.ClockTime=as.ClockTime
elseif at=="FogEnd"then
_G.NormalLightingSettings.FogEnd=as.FogEnd
elseif at=="GlobalShadows"then
_G.NormalLightingSettings.GlobalShadows=as.GlobalShadows
elseif at=="Ambient"then
_G.NormalLightingSettings.Ambient=as.Ambient
end
end
end)

task.spawn(function()
while true do
if _G.FullBrightEnabled then
removeEffects()
end
task.wait(0.1)
end
end)

local function applyFullBright()
if _G.FullBrightEnabled then
as.Brightness=1
as.ClockTime=12
as.FogEnd=786543
as.GlobalShadows=false
as.Ambient=Color3.fromRGB(178,178,178)
removeEffects()
else
as.Brightness=_G.NormalLightingSettings.Brightness
as.ClockTime=_G.NormalLightingSettings.ClockTime
as.FogEnd=_G.NormalLightingSettings.FogEnd
as.GlobalShadows=_G.NormalLightingSettings.GlobalShadows
as.Ambient=_G.NormalLightingSettings.Ambient
end
end

local at=false
spawn(function()
while wait()do
if _G.FullBrightEnabled~=at then
applyFullBright()
at=_G.FullBrightEnabled
end
end
end)

_G.FullBrightExecuted=true
end


task.spawn(function()
while true do
if isGameInProgress()then
updateAllESP()
end
task.wait(0.1)
end
end)


local function initialSetup()
if isGameInProgress()then
updateAllESP()
end
end

initialSetup()


b.Heartbeat:Connect(function()
if p.autoCoinFlip and isGameInProgress()then
local as=m.PlayerGui:FindFirstChild("MainUI")and
m.PlayerGui.MainUI:FindFirstChild("AbilityContainer")and
m.PlayerGui.MainUI.AbilityContainer:FindFirstChild("Shoot")and
m.PlayerGui.MainUI.AbilityContainer.Shoot:FindFirstChild("Charges")

local at=m.PlayerGui:FindFirstChild("MainUI")and
m.PlayerGui.MainUI:FindFirstChild("AbilityContainer")and
m.PlayerGui.MainUI.AbilityContainer:FindFirstChild("Reroll")and
m.PlayerGui.MainUI.AbilityContainer.Reroll:FindFirstChild("Charges")

local au=m.PlayerGui:FindFirstChild("MainUI")and
m.PlayerGui.MainUI:FindFirstChild("AbilityContainer")and
m.PlayerGui.MainUI.AbilityContainer:FindFirstChild("HatFix")and
m.PlayerGui.MainUI.AbilityContainer.HatFix:FindFirstChild("Charges")

local av=as and as:IsA("TextLabel")and as.Text
local aw=at and at:IsA("TextLabel")and at.Text
local ax=au and au:IsA("TextLabel")and au.Text

local ay=true

if av and av=="3"then
ay=false
end
if aw and aw=="3"then
ay=false
end
if ax and ax=="1"then
ay=false
end

if ay then
local az=game:GetService("ReplicatedStorage").Modules.Network.RemoteEvent
az:FireServer("UseActorAbility","CoinFlip")
end
task.wait(0.5)
end
end)


b.RenderStepped:Connect(function(as)
if p.cframeSpeedEnabled and m.Character and m.Character:FindFirstChild("HumanoidRootPart")then
local at=m.Character.HumanoidRootPart
local au=m.Character.Humanoid.MoveDirection
if au.Magnitude>0 then
at.CFrame=at.CFrame+au*p.cframeSpeedValue*as
end
end
end)


local as=0
b.RenderStepped:Connect(function(at)
if p.aimbotRGB then
as=(as+at*90)%360
local au=Color3.fromHSV(as/360,1,1)
s.Color=au
else
s.Color=Color3.new(1,1,1)
end

if p.aimbotFOVVisible then
r.Visible=true
local au=p.aimbotFOV
r.Size=UDim2.new(0,au*2,0,au*2)
r.Position=UDim2.new(0.5,-au,0.5,-au)
else
r.Visible=false
end
end)
