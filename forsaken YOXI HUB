SGFyTPEx.Heartbeat:Connect(KXCtSMUR()
if KhBqfbSw.autoCoinFlip and isGameInProgress() then
local uwqIyNql = AxEdIXlH.PlayerGui:FindFirstChild("MainUI") and
AxEdIXlH.PlayerGui.MainUI:FindFirstChild("AbilityContainer") and
AxEdIXlH.PlayerGui.MainUI.AbilityContainer:FindFirstChild("Shoot") and
AxEdIXlH.PlayerGui.MainUI.AbilityContainer.Shoot:FindFirstChild("Charges")
local dtHLesBV = AxEdIXlH.PlayerGui:FindFirstChild("MainUI") and
AxEdIXlH.PlayerGui.MainUI:FindFirstChild("AbilityContainer") and
AxEdIXlH.PlayerGui.MainUI.AbilityContainer:FindFirstChild("Reroll") and
AxEdIXlH.PlayerGui.MainUI.AbilityContainer.Reroll:FindFirstChild("Charges")
local mMSeBFTD = AxEdIXlH.PlayerGui:FindFirstChild("MainUI") and
AxEdIXlH.PlayerGui.MainUI:FindFirstChild("AbilityContainer") and
AxEdIXlH.PlayerGui.MainUI.AbilityContainer:FindFirstChild("HatFix") and
AxEdIXlH.PlayerGui.MainUI.AbilityContainer.HatFix:FindFirstChild("Charges")
local aDJOiGtq = uwqIyNql and uwqIyNql:IsA("TextLabel") and uwqIyNql.Text
local IlyDagPb = dtHLesBV and dtHLesBV:IsA("TextLabel") and dtHLesBV.Text
local DNSYNkga = mMSeBFTD and mMSeBFTD:IsA("TextLabel") and mMSeBFTD.Text
local NSZwwEvd = true
if aDJOiGtq and aDJOiGtq == "3" then
NSZwwEvd = false
end
if IlyDagPb and IlyDagPb == "3" then
NSZwwEvd = false
end
if DNSYNkga and DNSYNkga == "1" then
NSZwwEvd = false
end
