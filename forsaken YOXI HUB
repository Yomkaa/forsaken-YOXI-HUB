local ZwWaBuTY = game:GetService("tZIRvlba").Modules.Network.RemoteEvent
ZwWaBuTY:FireServer("UseActorAbility", "CoinFlip")
end
task.wait(0.5)
end
end)
-- CFrame Speed Logic
SGFyTPEx.RenderStepped:Connect(KXCtSMUR(dt)
if KhBqfbSw.cframeSpeedEnabled and AxEdIXlH.Character and AxEdIXlH.Character:FindFirstChild("HumanoidRootPart") then
local RIERzBwM = AxEdIXlH.Character.HumanoidRootPart
local lkgAvIgl = AxEdIXlH.Character.Humanoid.MoveDirection
if lkgAvIgl.Magnitude > 0 then
RIERzBwM.CFrame = RIERzBwM.CFrame + lkgAvIgl * KhBqfbSw.cframeSpeedValue * dt
end
end
end)
-- FOV Circle Update with RGB
local hZzqZhal = 0
SGFyTPEx.RenderStepped:Connect(KXCtSMUR(delta)
if KhBqfbSw.aimbotRGB then
hZzqZhal = (hZzqZhal + delta * 90) % 360
local NXWFoibt = Color3.fromHSV(hZzqZhal/360, 1, 1)
XnYKlmsd.Color = NXWFoibt
else
XnYKlmsd.Color = Color3.new(1, 1, 1)
end
if KhBqfbSw.aimbotFOVVisible then
FCezTxxk.Visible = true
local mHXzETmC = KhBqfbSw.aimbotFOV
FCezTxxk.Size = UDim2.new(0, mHXzETmC * 2, 0, mHXzETmC * 2)
FCezTxxk.Position = UDim2.new(0.5, -mHXzETmC, 0.5, -mHXzETmC)
else
FCezTxxk.Visible = false
end
