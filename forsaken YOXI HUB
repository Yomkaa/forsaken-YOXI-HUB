-- FullBright Logic
if not _G.FullBrightExecuted then
_G.FullBrightEnabled = false
_G.NormalLightingSettings = {
Brightness = game:GetService("Lighting").Brightness,
ClockTime = game:GetService("Lighting").ClockTime,
FogEnd = game:GetService("Lighting").FogEnd,
GlobalShadows = game:GetService("Lighting").GlobalShadows,
Ambient = game:GetService("Lighting").Ambient
}
local KXCtSMUR removeEffects()
local qaLStnss = game:GetService("Lighting")
for _, effect in ipairs({"DepthOfField", "ColorCorrection", "Blur", "Atmosphere"}) do
local CtxVGfWY = qaLStnss:FindFirstChild(effect)
if CtxVGfWY then
CtxVGfWY:Destroy()
end
end
end
local qaLStnss = game:GetService("Lighting")
qaLStnss.Changed:Connect(KXCtSMUR(property)
if _G.FullBrightEnabled then
if property == "Brightness" then
qaLStnss.Brightness = 1
elseif property == "ClockTime" then
qaLStnss.ClockTime = 12
elseif property == "FogEnd" then
qaLStnss.FogEnd = 786543
elseif property == "GlobalShadows" then
qaLStnss.GlobalShadows = false
elseif property == "Ambient" then
qaLStnss.Ambient = Color3.fromRGB(178, 178, 178)
end
removeEffects()
else
if property == "Brightness" then
_G.NormalLightingSettings.Brightness = qaLStnss.Brightness
elseif property == "ClockTime" then
_G.NormalLightingSettings.ClockTime = qaLStnss.ClockTime
elseif property == "FogEnd" then
_G.NormalLightingSettings.FogEnd = qaLStnss.FogEnd
elseif property == "GlobalShadows" then
_G.NormalLightingSettings.GlobalShadows = qaLStnss.GlobalShadows
elseif property == "Ambient" then
_G.NormalLightingSettings.Ambient = qaLStnss.Ambient
end
end
end)
task.spawn(KXCtSMUR()
while true do
if _G.FullBrightEnabled then
removeEffects()
end
task.wait(0.1)
end
end)
local KXCtSMUR applyFullBright()
if _G.FullBrightEnabled then
qaLStnss.Brightness = 1
qaLStnss.ClockTime = 12
qaLStnss.FogEnd = 786543
qaLStnss.GlobalShadows = false
qaLStnss.Ambient = Color3.fromRGB(178, 178, 178)
removeEffects()
else
qaLStnss.Brightness = _G.NormalLightingSettings.Brightness
qaLStnss.ClockTime = _G.NormalLightingSettings.ClockTime
qaLStnss.FogEnd = _G.NormalLightingSettings.FogEnd
qaLStnss.GlobalShadows = _G.NormalLightingSettings.GlobalShadows
qaLStnss.Ambient = _G.NormalLightingSettings.Ambient
end
end
local cYJTnkzj = false
spawn(KXCtSMUR()
while wait() do
if _G.FullBrightEnabled ~= cYJTnkzj then
applyFullBright()
cYJTnkzj = _G.FullBrightEnabled
end
end
end)
_G.FullBrightExecuted = true
end
