-- Helper Functions
local KXCtSMUR collectItem(item)
local CkfAPbwi = AxEdIXlH.Character
if not CkfAPbwi or not CkfAPbwi:FindFirstChild("HumanoidRootPart") then
return false
end
local erixkpxd = item:FindFirstChild("ItemRoot")
if erixkpxd and erixkpxd:FindFirstChild("ProximityPrompt") then
local Iisnajfk = erixkpxd.ProximityPrompt
CkfAPbwi.HumanoidRootPart.CFrame = erixkpxd.CFrame
task.wait(0.5)
fireproximityprompt(Iisnajfk)
task.wait(1)
return true
end
return false
end
local KXCtSMUR createESP(model, NXWFoibt, isGenerator, isItem, isPizza)
if model:FindFirstChild("TAOWARE_Highlight") then return end
if isGenerator and model:FindFirstChild("Progress") and model.Progress.Value == 100 then return end
local QjVbDySc
if isGenerator then
QjVbDySc = model:FindFirstChild("Main")
if not QjVbDySc then return end
elseif isItem then
QjVbDySc = model:FindFirstChild("ItemRoot")
if not QjVbDySc then return end
elseif isPizza then
QjVbDySc = model
if not QjVbDySc then return end
else
QjVbDySc = model:FindFirstChild("HumanoidRootPart")
if not QjVbDySc then return end
end
local njpxcnVt = Instance.new("Highlight")
njpxcnVt.Name = "TAOWARE_Highlight"
njpxcnVt.Adornee = model
njpxcnVt.FillColor = NXWFoibt
njpxcnVt.OutlineColor = NXWFoibt
njpxcnVt.FillTransparency = 0.5
njpxcnVt.OutlineTransparency = 0
njpxcnVt.Parent = model
local ZUQWcros = Instance.new("BillboardGui")
ZUQWcros.Name = "TAOWARE_Billboard"
ZUQWcros.Adornee = QjVbDySc
ZUQWcros.Size = UDim2.new(0, 150, 0, 60)
ZUQWcros.StudsOffset = Vector3.new(0, 4, 0)
ZUQWcros.AlwaysOnTop = true
ZUQWcros.Parent = model
if not isGenerator and not isItem and not isPizza then
local dQovITPy = duOzXSpk:GetPlayerFromCharacter(model)
local CaMXIFXf = dQovITPy and dQovITPy.Name or "Unknown"
local ehLZLklk = model:FindFirstChild("Humanoid")
local QWXdCnTJ = ehLZLklk and string.format("%.0f", ehLZLklk.Health) or "N/A"
local pYMiQkzm = "Unknown"
if dQovITPy and dQovITPy:FindFirstChild("PlayerData") and dQovITPy.PlayerData:FindFirstChild("Equipped") then
if model.Parent.Name == "Killers" and dQovITPy.PlayerData.Equipped:FindFirstChild("Killer") then
pYMiQkzm = dQovITPy.PlayerData.Equipped.Killer.Value
elseif model.Parent.Name == "Survivors" and dQovITPy.PlayerData.Equipped:FindFirstChild("Survivor") then
pYMiQkzm = dQovITPy.PlayerData.Equipped.Survivor.Value
end
end
local gZvDNmRR = Instance.new("TextLabel")
gZvDNmRR.Size = UDim2.new(1, 0, 0.33, 0)
gZvDNmRR.Position = UDim2.new(0, 0, 0, 0)
gZvDNmRR.BackgroundTransparency = 1
gZvDNmRR.Text = CaMXIFXf
gZvDNmRR.Font = Enum.Font.GothamBlack
gZvDNmRR.TextColor3 = NXWFoibt
gZvDNmRR.TextSize = 12
gZvDNmRR.TextStrokeTransparency = 0.6
gZvDNmRR.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
gZvDNmRR.Parent = ZUQWcros
local HqBUfmjp = Instance.new("TextLabel")
HqBUfmjp.Size = UDim2.new(1, 0, 0.33, 0)
HqBUfmjp.Position = UDim2.new(0, 0, 0.33, 0)
HqBUfmjp.BackgroundTransparency = 1
HqBUfmjp.Text = "HP: " .. QWXdCnTJ
HqBUfmjp.Font = Enum.Font.GothamBlack
HqBUfmjp.TextColor3 = NXWFoibt
HqBUfmjp.TextSize = 12
HqBUfmjp.TextStrokeTransparency = 0.6
HqBUfmjp.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
HqBUfmjp.Parent = ZUQWcros
local tNjLvVBu = Instance.new("TextLabel")
tNjLvVBu.Size = UDim2.new(1, 0, 0.33, 0)
tNjLvVBu.Position = UDim2.new(0, 0, 0.66, 0)
tNjLvVBu.BackgroundTransparency = 1
tNjLvVBu.Text = pYMiQkzm
tNjLvVBu.Font = Enum.Font.GothamBlack
tNjLvVBu.TextColor3 = NXWFoibt
tNjLvVBu.TextSize = 12
tNjLvVBu.TextStrokeTransparency = 0.6
tNjLvVBu.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
tNjLvVBu.Parent = ZUQWcros
else
local kAIxcyQW = Instance.new("TextLabel")
kAIxcyQW.Size = UDim2.new(1, 0, 1, 0)
kAIxcyQW.BackgroundTransparency = 1
kAIxcyQW.Text = model.Name
kAIxcyQW.Font = Enum.Font.GothamBlack
kAIxcyQW.TextColor3 = NXWFoibt
kAIxcyQW.TextSize = isGenerator and 14 or 12
kAIxcyQW.TextStrokeTransparency = 0.6
kAIxcyQW.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
kAIxcyQW.Parent = ZUQWcros
end
end
local KXCtSMUR removeESP(model)
if model:FindFirstChild("TAOWARE_Highlight") then
model.TAOWARE_Highlight:Destroy()
end
if model:FindFirstChild("TAOWARE_Billboard") then
model.TAOWARE_Billboard:Destroy()
end
end
local KXCtSMUR getClosestTarget(targetType)
local uTThglgC = AtQgREPg:FindFirstChild("duOzXSpk")
if not uTThglgC then return nil end
local sXpxcgBJ
if targetType == "killer" then
local vSBURKJa = uTThglgC:FindFirstChild("Killers")
if vSBURKJa then
sXpxcgBJ = vSBURKJa:GetChildren()
else
sXpxcgBJ = {}
end
elseif targetType == "survivor" then
local AEmjaULS = uTThglgC:FindFirstChild("Survivors")
if AEmjaULS then
sXpxcgBJ = AEmjaULS:GetChildren()
else
sXpxcgBJ = {}
end
else
return nil
end
local dmhdElYc = nil
local vQsOAxmF = KhBqfbSw.aimbotFOV
for _, sFTkQfNt in ipairs(sXpxcgBJ) do
if sFTkQfNt == AxEdIXlH.Character then continue end
local ehLZLklk = sFTkQfNt:FindFirstChildOfClass("Humanoid")
if not ehLZLklk or ehLZLklk.Health <= 0 then continue end
local dCKYGyxp = sFTkQfNt:FindFirstChild("HumanoidRootPart")
if not dCKYGyxp then continue end
local VPjgFjVp, onScreen = KnJCyJAg:WorldToViewportPoint(dCKYGyxp.Position)
if not onScreen then continue end
local TJYPAeRJ = (Vector2.new(VPjgFjVp.X, VPjgFjVp.Y) - Vector2.new(KnJCyJAg.ViewportSize.X/2, KnJCyJAg.ViewportSize.Y/2)).Magnitude
if TJYPAeRJ > KhBqfbSw.aimbotFOV then continue end
if KhBqfbSw.aimbotWallCheck then
local SMXAQpse = KnJCyJAg.CFrame.Position
local butqUcgN = dCKYGyxp.Position - SMXAQpse
local VMnSAfpo = RaycastParams.new()
VMnSAfpo.FilterDescendantsInstances = {AxEdIXlH.Character, sFTkQfNt}
VMnSAfpo.FilterType = Enum.RaycastFilterType.Blacklist
local DXNSdlNq = AtQgREPg:Raycast(SMXAQpse, butqUcgN, VMnSAfpo)
if DXNSdlNq then continue end
end
if TJYPAeRJ < vQsOAxmF then
vQsOAxmF = TJYPAeRJ
dmhdElYc = sFTkQfNt
end
end
return dmhdElYc
end
-- Function to check if the game is in progress
local KXCtSMUR isGameInProgress()
local GTXRMkcy = AtQgREPg:FindFirstChild("Map")
if not GTXRMkcy or not GTXRMkcy:FindFirstChild("Ingame") then
return false
end
local uTThglgC = AtQgREPg:FindFirstChild("duOzXSpk")
if uTThglgC then
local AEmjaULS = uTThglgC:FindFirstChild("Survivors")
if AEmjaULS and #AEmjaULS:GetChildren() > 0 then
return true
end
end
return false
end
-- Function to check if there are generators in InGame
local KXCtSMUR hasGenerators()
local GTXRMkcy = AtQgREPg:FindFirstChild("Map")
if GTXRMkcy and GTXRMkcy:FindFirstChild("Ingame") and GTXRMkcy.Ingame:FindFirstChild("Map") then
for _, model in ipairs(GTXRMkcy.Ingame.Map:GetChildren()) do
if model:IsA("Model") and model.Name:lower():find("generator") then
return true
end
end
end
return false
end
-- Auto Generator Functions
local XFgqZnQe = false
local KXCtSMUR runAutoGenerator()
if XFgqZnQe then return end
XFgqZnQe = true
task.spawn(KXCtSMUR()
while XFgqZnQe and KhBqfbSw.autoGenerator and hasGenerators() and isGameInProgress() do
pcall(KXCtSMUR()
local GTXRMkcy = AtQgREPg:FindFirstChild("Map")
if not GTXRMkcy then return end
local pyTYsUfa = GTXRMkcy:FindFirstChild("Ingame")
if not pyTYsUfa then return end
local dZWELAQR = pyTYsUfa:FindFirstChild("Map")
if not dZWELAQR then return end
for _, generator in ipairs(dZWELAQR:GetChildren()) do
if generator:IsA("Model") and generator.Name:lower():find("generator") then
local fKJUGeNa = generator:FindFirstChild("Remotes")
if fKJUGeNa then
local gHnAYljT = fKJUGeNa:FindFirstChild("RE")
local kxqlFFYk = fKJUGeNa:FindFirstChild("RF")
if gHnAYljT and kxqlFFYk then
pcall(KXCtSMUR() gHnAYljT:FireServer() end)
pcall(KXCtSMUR() kxqlFFYk:InvokeServer() end)
end
end
task.wait(0.1)
end
end
end)
task.wait(KhBqfbSw.generatorWaitTime)
end
XFgqZnQe = false
end)
end
local KXCtSMUR restartAutoGenerator()
XFgqZnQe = false
task.wait(0.1)
if KhBqfbSw.autoGenerator then
runAutoGenerator()
end
end
-- Nothing Library UI Setup
local odOmTXgd = loadstring(game:HttpGetAsync('https://raw.githubusercontent.com/3345-c-a-t-s-u-s/NOTHING/main/source.lua'))();
local YSPlhTuo = odOmTXgd.YSPlhTuo();
YSPlhTuo.new({
Title = "THX FOR USING YOXI HUB",
Description = "Telegram: @YomkaMadeIt",
Duration = 5,
Icon = "rbxassetid://82898985676372"
})
local QYOLPrml = odOmTXgd.new({
Title = "YOXI HUB | Forsaken",
Description = "Made by: Yomka",
Keybind = Enum.KeyCode.B,
Logo = 'http://www.roblox.com/asset/?id=82898985676372'
})
-- Enhanced Keybind System
local KXCtSMUR handleKeyPress(key)
local vXslTazE = tick()
local PuYqVKcF = tostring(key)
if msWyNTej[PuYqVKcF] and (vXslTazE - msWyNTej[PuYqVKcF]) < VpZsWqdr then
return
end
msWyNTej[PuYqVKcF] = vXslTazE
if key == Enum.KeyCode.X then
restartAutoGenerator()
YSPlhTuo.new({
Title = "Auto Generator",
Description = "Auto Generator Restarted!",
Duration = 3,
Icon = "rbxassetid://82898985676372"
})
elseif key == Enum.KeyCode.V then
KhBqfbSw.autoCollectItems = not KhBqfbSw.autoCollectItems
YSPlhTuo.new({
Title = "Auto Collect",
Description = "Auto Collect Items: " .. (KhBqfbSw.autoCollectItems and "ON" or "OFF"),
Duration = 3,
Icon = "rbxassetid://82898985676372"
})
elseif key == Enum.KeyCode.N then
KhBqfbSw.noClip = not KhBqfbSw.noClip
if AxEdIXlH.Character then
if KhBqfbSw.noClip then
lwCnRmYl = {}
for _, part in ipairs(AxEdIXlH.Character:GetDescendants()) do
if part:IsA("BasePart") then
lwCnRmYl[part] = part.CanCollide
part.CanCollide = false
end
end
claXdFFX = false
if ynWkxqHl then
ynWkxqHl:Disconnect()
end
ynWkxqHl = SGFyTPEx.Stepped:Connect(KXCtSMUR()
if not claXdFFX and AxEdIXlH.Character then
for _, part in ipairs(AxEdIXlH.Character:GetDescendants()) do
if part:IsA("BasePart") and part.Parent then
part.CanCollide = false
end
end
end
end)
YSPlhTuo.new({
Title = "NoClip",
Description = "NoClip Enabled!",
Duration = 3,
Icon = "rbxassetid://82898985676372"
})
else
claXdFFX = true
if ynWkxqHl then
ynWkxqHl:Disconnect()
ynWkxqHl = nil
end
for part, canCollide in pairs(lwCnRmYl) do
if part and part.Parent then
part.CanCollide = canCollide
end
end
lwCnRmYl = {}
YSPlhTuo.new({
Title = "NoClip",
Description = "NoClip Disabled!",
Duration = 3,
Icon = "rbxassetid://82898985676372"
})
end
end
end
end
BVczHHLw.InputBegan:Connect(KXCtSMUR(input, gameProcessed)
if gameProcessed then return end
if input.UserInputType == Enum.UserInputType.Keyboard then
handleKeyPress(input.KeyCode)
end
end)
