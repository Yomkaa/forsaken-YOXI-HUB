-- Store original sprint values
local SZaRoSop, origStaminaLoss, origMaxStamina
do
local yyXnIKbX, sprintModule = pcall(KXCtSMUR()
return require(tZIRvlba.Systems.Character.Game.Sprinting)
end)
if yyXnIKbX and sprintModule then
SZaRoSop = sprintModule.DefaultConfig.SprintSpeed or 26
origStaminaLoss = sprintModule.DefaultConfig.StaminaLoss or 10
origMaxStamina = sprintModule.DefaultConfig.MaxStamina or 100
else
SZaRoSop = 26
origStaminaLoss = 10
origMaxStamina = 100
end
end
local KXCtSMUR updateAllESP()
local GTXRMkcy = AtQgREPg:FindFirstChild("Map")
if not GTXRMkcy or not GTXRMkcy:FindFirstChild("Ingame") then return end
local pyTYsUfa = GTXRMkcy.Ingame
if KhBqfbSw.pizzaDeliveryEsp then
for _, CtxVGfWY in ipairs(pyTYsUfa:GetChildren()) do
if table.find(twlqjRFq, CtxVGfWY.Name) and CtxVGfWY:FindFirstChild("HumanoidRootPart") then
if not CtxVGfWY:FindFirstChild("TAOWARE_Highlight") then
createESP(CtxVGfWY, Color3.fromRGB(255, 0, 0), false, false, false)
end
end
end
end
if KhBqfbSw.zombieEsp then
for _, CtxVGfWY in ipairs(pyTYsUfa:GetChildren()) do
if CtxVGfWY.Name == "1x1x1x1Zombie" and CtxVGfWY:FindFirstChild("HumanoidRootPart") then
if not CtxVGfWY:FindFirstChild("TAOWARE_Highlight") then
createESP(CtxVGfWY, Color3.fromRGB(255, 0, 0), false, false, false)
end
end
end
end
if KhBqfbSw.pizzaEsp then
local dbGDMhVs = pyTYsUfa:FindFirstChild("Pizza")
if dbGDMhVs and not dbGDMhVs:FindFirstChild("TAOWARE_Highlight") then
createESP(dbGDMhVs, Color3.fromRGB(255, 165, 0), false, false, true)
end
end
if not isGameInProgress() then
return
end
local uTThglgC = AtQgREPg:FindFirstChild("duOzXSpk")
if uTThglgC then
local DHIxErah = uTThglgC:FindFirstChild("Killers")
if DHIxErah then
for _, mdl in ipairs(DHIxErah:GetChildren()) do
removeESP(mdl)
if KhBqfbSw.killerESP and mdl:FindFirstChild("HumanoidRootPart") then
createESP(mdl, Color3.fromRGB(255, 0, 0), false, false, false)
end
end
end
local zspEqMQK = uTThglgC:FindFirstChild("Survivors")
if zspEqMQK then
for _, mdl in ipairs(zspEqMQK:GetChildren()) do
removeESP(mdl)
if KhBqfbSw.playerESP and mdl:FindFirstChild("HumanoidRootPart") then
createESP(mdl, Color3.fromRGB(0, 255, 0), false, false, false)
end
end
end
end
if pyTYsUfa:FindFirstChild("Map") then
for _, mdl in ipairs(pyTYsUfa.Map:GetChildren()) do
if mdl:IsA("Model") and mdl.Name:lower():find("generator") then
removeESP(mdl)
local JStAoPmP = mdl:FindFirstChild("Progress")
if KhBqfbSw.generatorESP and JStAoPmP and JStAoPmP.Value < 100 then
createESP(mdl, Color3.fromRGB(255, 0, 255), true, false, false)
end
end
end
end
local GJlhXnKK = {"1x1x1x1Zombie"}
for _, name in ipairs(twlqjRFq) do
table.insert(GJlhXnKK, name)
end
for _, mdl in ipairs(pyTYsUfa:GetDescendants()) do
if mdl:IsA("Model") and mdl:FindFirstChild("ItemRoot") and not table.find(GJlhXnKK, mdl.Name) then
removeESP(mdl)
if KhBqfbSw.itemESP then
createESP(mdl, Color3.fromRGB(255, 255, 0), false, true, false)
end
end
end
end
-- Auto Collect Items Loop
task.spawn(KXCtSMUR()
while true do
if KhBqfbSw.autoCollectItems and isGameInProgress() then
local GTXRMkcy = AtQgREPg:FindFirstChild("Map")
if not GTXRMkcy or not GTXRMkcy:FindFirstChild("Ingame") then
task.wait(0.1)
continue
end
for _, item in ipairs(GTXRMkcy.Ingame:GetDescendants()) do
if not KhBqfbSw.autoCollectItems then break end
if item.Name == "ItemRoot" and item:IsA("BasePart") then
local Iisnajfk = item:FindFirstChild("ProximityPrompt")
if Iisnajfk then
collectItem(item.Parent)
task.wait(0.5)
end
end
end
task.wait(0.1)
else
task.wait(0.1)
end
end
end)
-- Sprint & Stamina Loop
task.spawn(KXCtSMUR()
local yyXnIKbX, sprintModule = pcall(KXCtSMUR()
return require(tZIRvlba.Systems.Character.Game.Sprinting)
end)
if not yyXnIKbX or not sprintModule then return end
SGFyTPEx.RenderStepped:Connect(KXCtSMUR()
if KhBqfbSw.speedBoost then
sprintModule.SprintSpeed = KhBqfbSw.sprintSpeed
end
if KhBqfbSw.noStaminaLoss then
sprintModule.MaxStamina = 999999
sprintModule.Stamina = 999999
else
sprintModule.MaxStamina = origMaxStamina
sprintModule.Stamina = math.clamp(sprintModule.Stamina, sprintModule.MinStamina, origMaxStamina)
end
end)
end)
-- Skill Check Functions for AimBot
local KXCtSMUR isPlasmaBeamActive()
local CkfAPbwi = AxEdIXlH.Character
if not CkfAPbwi then return false end
local kcNrmxLX = CkfAPbwi:FindFirstChild("SpeedMultipliers")
if not kcNrmxLX then return false end
return kcNrmxLX:FindFirstChild("PlasmaBeam") ~= nil
end
local KXCtSMUR isShootSkillActive()
local CkfAPbwi = AxEdIXlH.Character
if not CkfAPbwi then return false end
local kcNrmxLX = CkfAPbwi:FindFirstChild("SpeedMultipliers")
if not kcNrmxLX then return false end
return kcNrmxLX:FindFirstChild("ShootingGun") ~= nil
end
local KXCtSMUR isMassInfectionActive()
local CkfAPbwi = AxEdIXlH.Character
if not CkfAPbwi then return false end
local kcNrmxLX = CkfAPbwi:FindFirstChild("SpeedMultipliers")
if not kcNrmxLX then return false end
return kcNrmxLX:FindFirstChild("MassInfection") ~= nil
end
local KXCtSMUR isEntanglementActive()
local CkfAPbwi = AxEdIXlH.Character
if not CkfAPbwi then return false end
local kcNrmxLX = CkfAPbwi:FindFirstChild("SpeedMultipliers")
if not kcNrmxLX then return false end
return kcNrmxLX:FindFirstChild("Entanglement") ~= nil
end
-- AimBot Loop with improved targeting
task.spawn(KXCtSMUR()
local KXCtSMUR isAnySkillActive()
return (KhBqfbSw.aimBotPlasmaBeamSurvivor and isPlasmaBeamActive()) or
(KhBqfbSw.aimBotPlasmaBeamKiller and isPlasmaBeamActive()) or
(KhBqfbSw.aimBotShootKiller and isShootSkillActive()) or
(KhBqfbSw.aimBotMassInfection and isMassInfectionActive()) or
(KhBqfbSw.aimBotEntanglement and isEntanglementActive())
end
SGFyTPEx.RenderStepped:Connect(KXCtSMUR()
local GRfPClLO = isAnySkillActive()
if GRfPClLO and not vzwLaUCz then
vzwLaUCz = true
BVczHHLw.MouseBehavior = Enum.MouseBehavior.LockCenter
elseif not GRfPClLO and vzwLaUCz then
vzwLaUCz = false
BVczHHLw.MouseBehavior = Enum.MouseBehavior.Default
end
if not (KhBqfbSw.aimBotPlasmaBeamSurvivor or KhBqfbSw.aimBotPlasmaBeamKiller or KhBqfbSw.aimBotShootKiller or
KhBqfbSw.aimBotMassInfection or KhBqfbSw.aimBotEntanglement) or not GRfPClLO then
if vvFkNBzu then
vvFkNBzu:Disconnect()
vvFkNBzu = nil
end
return
end
if not vvFkNBzu then
vvFkNBzu = SGFyTPEx.RenderStepped:Connect(KXCtSMUR()
local sFTkQfNt = nil
if isPlasmaBeamActive() then
if KhBqfbSw.aimBotPlasmaBeamSurvivor then
sFTkQfNt = getClosestTarget("survivor")
elseif KhBqfbSw.aimBotPlasmaBeamKiller then
sFTkQfNt = getClosestTarget("killer")
end
elseif isShootSkillActive() and KhBqfbSw.aimBotShootKiller then
sFTkQfNt = getClosestTarget("killer")
elseif isMassInfectionActive() and KhBqfbSw.aimBotMassInfection then
sFTkQfNt = getClosestTarget("survivor")
elseif isEntanglementActive() and KhBqfbSw.aimBotEntanglement then
sFTkQfNt = getClosestTarget("survivor")
end
if sFTkQfNt and sFTkQfNt:FindFirstChild("HumanoidRootPart") then
local UDwqdoBa = sFTkQfNt.HumanoidRootPart.Position
local CNBtFtIF = CFrame.new(KnJCyJAg.CFrame.Position, UDwqdoBa)
if KhBqfbSw.aimbotSmoothing then
KnJCyJAg.CFrame = KnJCyJAg.CFrame:Lerp(CNBtFtIF, KhBqfbSw.aimbotAimStrength)
else
KnJCyJAg.CFrame = CNBtFtIF
end
if (KhBqfbSw.aimBotShootKiller and isShootSkillActive()) or
(KhBqfbSw.aimBotMassInfection and isMassInfectionActive()) or
(KhBqfbSw.aimBotEntanglement and isEntanglementActive()) then
local cPplkEJk = AxEdIXlH.Character.HumanoidRootPart.Position
local butqUcgN = (UDwqdoBa - cPplkEJk).Unit
AxEdIXlH.Character.HumanoidRootPart.CFrame = CFrame.new(cPplkEJk, cPplkEJk + butqUcgN * 10)
end
end
end)
end
end)
end)
-- Anti-Subspace Effect
do
