local ak,al,am
do
local an,ao=pcall(function()
return require(c.Systems.Character.Game.Sprinting)
end)
if an and ao then
ak=ao.DefaultConfig.SprintSpeed or 26
al=ao.DefaultConfig.StaminaLoss or 10
am=ao.DefaultConfig.MaxStamina or 100
else
ak=26
al=10
am=100
end
end

local function updateAllESP()
local an=d:FindFirstChild"Map"
if not an or not an:FindFirstChild"Ingame"then return end
local ao=an.Ingame

if k.pizzaDeliveryEsp then
for ap,aq in ipairs(ao:GetChildren())do
if table.find(x,aq.Name)and aq:FindFirstChild"HumanoidRootPart"then
if not aq:FindFirstChild"TAOWARE_Highlight"then
createESP(aq,Color3.fromRGB(255,0,0),false,false,false)
end
end
end
end

if k.zombieEsp then
for ap,aq in ipairs(ao:GetChildren())do
if aq.Name=="1x1x1x1Zombie"and aq:FindFirstChild"HumanoidRootPart"then
if not aq:FindFirstChild"TAOWARE_Highlight"then
createESP(aq,Color3.fromRGB(255,0,0),false,false,false)
end
end
end
end

if k.pizzaEsp then
local ap=ao:FindFirstChild"Pizza"
if ap and not ap:FindFirstChild"TAOWARE_Highlight"then
createESP(ap,Color3.fromRGB(255,165,0),false,false,true)
end
end

if not isGameInProgress()then
return
end

local ap=d:FindFirstChild"Players"
if ap then
local aq=ap:FindFirstChild"Killers"
if aq then
for ar,as in ipairs(aq:GetChildren())do
removeESP(as)
if k.killerESP and as:FindFirstChild"HumanoidRootPart"then
createESP(as,Color3.fromRGB(255,0,0),false,false,false)
end
end
end

local ar=ap:FindFirstChild"Survivors"
if ar then
for as,at in ipairs(ar:GetChildren())do
removeESP(at)
if k.playerESP and at:FindFirstChild"HumanoidRootPart"then
createESP(at,Color3.fromRGB(0,255,0),false,false,false)
end
end
end
end

if ao:FindFirstChild"Map"then
for aq,ar in ipairs(ao.Map:GetChildren())do
if ar:IsA"Model"and ar.Name:lower():find"generator"then
removeESP(ar)
local as=ar:FindFirstChild"Progress"
if k.generatorESP and as and as.Value<100 then
createESP(ar,Color3.fromRGB(255,0,255),true,false,false)
end
end
end
end

local aq={"1x1x1x1Zombie"}
for ar,as in ipairs(x)do
table.insert(aq,as)
end
for ar,as in ipairs(ao:GetDescendants())do
if as:IsA"Model"and as:FindFirstChild"ItemRoot"and not table.find(aq,as.Name)then
removeESP(as)
if k.itemESP then
createESP(as,Color3.fromRGB(255,255,0),false,true,false)
end
end
end
end


task.spawn(function()
while true do
if k.autoCollectItems and isGameInProgress()then
local an=d:FindFirstChild"Map"
if not an or not an:FindFirstChild"Ingame"then
task.wait(0.1)
continue
end
for ao,ap in ipairs(an.Ingame:GetDescendants())do
if not k.autoCollectItems then break end
if ap.Name=="ItemRoot"and ap:IsA"BasePart"then
local aq=ap:FindFirstChild"ProximityPrompt"
if aq then
collectItem(ap.Parent)
task.wait(0.5)
end
end
end
task.wait(0.1)
else
task.wait(0.1)
end
end
end)


task.spawn(function()
local an,ao=pcall(function()
return require(c.Systems.Character.Game.Sprinting)
end)
if not an or not ao then return end

b.RenderStepped:Connect(function()
if k.speedBoost then
ao.SprintSpeed=k.sprintSpeed
end
if k.noStaminaLoss then
ao.MaxStamina=999999
ao.Stamina=999999
else
ao.MaxStamina=am
ao.Stamina=math.clamp(ao.Stamina,ao.MinStamina,am)
end
end)
end)


local function isPlasmaBeamActive()
local an=h.Character
if not an then return false end
local ao=an:FindFirstChild"SpeedMultipliers"
if not ao then return false end
return ao:FindFirstChild"PlasmaBeam"~=nil
end

local function isShootSkillActive()
local an=h.Character
if not an then return false end
local ao=an:FindFirstChild"SpeedMultipliers"
if not ao then return false end
return ao:FindFirstChild"ShootingGun"~=nil
end

local function isMassInfectionActive()
local an=h.Character
if not an then return false end
local ao=an:FindFirstChild"SpeedMultipliers"
if not ao then return false end
return ao:FindFirstChild"MassInfection"~=nil
end

local function isEntanglementActive()
local an=h.Character
if not an then return false end
local ao=an:FindFirstChild"SpeedMultipliers"
if not ao then return false end
return ao:FindFirstChild"Entanglement"~=nil
end


task.spawn(function()
local function isAnySkillActive()
return(k.aimBotPlasmaBeamSurvivor and isPlasmaBeamActive())or
(k.aimBotPlasmaBeamKiller and isPlasmaBeamActive())or
(k.aimBotShootKiller and isShootSkillActive())or
(k.aimBotMassInfection and isMassInfectionActive())or
(k.aimBotEntanglement and isEntanglementActive())
end

b.RenderStepped:Connect(function()
local an=isAnySkillActive()
if an and not u then
u=true
f.MouseBehavior=Enum.MouseBehavior.LockCenter
elseif not an and u then
u=false
f.MouseBehavior=Enum.MouseBehavior.Default
end

if not(k.aimBotPlasmaBeamSurvivor or k.aimBotPlasmaBeamKiller or k.aimBotShootKiller or
k.aimBotMassInfection or k.aimBotEntanglement)or not an then
if t then
t:Disconnect()
t=nil
end
return
end

if not t then
t=b.RenderStepped:Connect(function()
local ao=nil
if isPlasmaBeamActive()then
if k.aimBotPlasmaBeamSurvivor then
ao=getClosestTarget"survivor"
elseif k.aimBotPlasmaBeamKiller then
ao=getClosestTarget"killer"
end
elseif isShootSkillActive()and k.aimBotShootKiller then
ao=getClosestTarget"killer"
elseif isMassInfectionActive()and k.aimBotMassInfection then
ao=getClosestTarget"survivor"
elseif isEntanglementActive()and k.aimBotEntanglement then
ao=getClosestTarget"survivor"
end

if ao and ao:FindFirstChild"HumanoidRootPart"then
local ap=ao.HumanoidRootPart.Position
local aq=CFrame.new(i.CFrame.Position,ap)
if k.aimbotSmoothing then
i.CFrame=i.CFrame:Lerp(aq,k.aimbotAimStrength)
else
i.CFrame=aq
end

if(k.aimBotShootKiller and isShootSkillActive())or
(k.aimBotMassInfection and isMassInfectionActive())or
(k.aimBotEntanglement and isEntanglementActive())then
local ar=h.Character.HumanoidRootPart.Position
local as=(ap-ar).Unit
h.Character.HumanoidRootPart.CFrame=CFrame.new(ar,ar+as*10)
end
end
end)
end
end)
end)


do
local an,ao=pcall(function()
return require(c.Modules.StatusEffects.SurvivorExclusive.Subspaced)
end)
if an and ao then
ao.Applied=function()end
ao.Removed=function()end
end
end


do
local an,ao=pcall(function()
return require(c.Modules.StatusEffects.KillerExclusive.Glitched)
end)
if an and ao then
local ap=ao.Applied
ao.Applied=function(aq)
if k.antiGlitchedEffect then
return
end
return ap(aq)
end
end
end


do
local an,ao=pcall(function()
return require(c.Modules.StatusEffects.Blindness)
end)
if an and ao then
local ap=ao.Applied
ao.Applied=function(aq)
if k.antiBlindness then
return
end
return ap(aq)
end
end
end


task.spawn(function()
while true do
local an=d:FindFirstChild"Players"
if an then
for ao,ap in ipairs(an:GetChildren())do
for aq,ar in ipairs(ap:GetChildren())do
if ar:IsA"Model"then
if k.antiWalkSpeedOverride then
local as=ar:FindFirstChild"Result"
if as then
as:Destroy()
end
end
local as=ar:FindFirstChild"SpeedMultipliers"
if as then
local at=as:FindFirstChild"HinderedMovement"
if at and(k.antiCorruptNature or k.antiPizzaDelivery)then
at.Value=1.8
end
if k.antiMassInfection then
local au=as:FindFirstChild"MassInfection"
if au then
au.Value=1.8
end
end
if k.antiEntanglement then
local au=as:FindFirstChild"Entanglement"
if au then
au.Value=1.8
end
end
local au=as:FindFirstChild"SlowedStatus"
if au then
au:Destroy()
end
local av=as:FindFirstChild"FallSlowness"
if av and k.antiFallDebuff then
av:Destroy()
end

if k.antiColaDebuff then
local aw=as:FindFirstChild"DrinkingCola"
if aw then
aw.Value=1.8
end
local ax=as:FindFirstChild"BloxyColaItem"
if ax then
ax.Value=1.8
end
end

if k.antiSlateskinDebuff then
local aw=as:FindFirstChild"DrinkingSlateskin"
if aw then
aw.Value=1.8
end
local ax=as:FindFirstChild"SlateskinStatus"
if ax then
ax.Value=1.8
end
end

if k.antiBurgerDebuff then
local aw=as:FindFirstChild"EatingGhostburger"
if aw then
aw.Value=1.8
end
end

if k.antiSlashDebuff then
local aw=as:FindFirstChild"StunningKiller"
if aw then
aw.Value=1.8
end
end

if k.antiChickenDebuff then
local aw=as:FindFirstChild"EatFriedChicken"
if aw then
aw.Value=1.8
end
end

if k.antiBlockDebuff then
local aw=as:FindFirstChild"GuestBlocking"
if aw then
aw.Value=1.8
end
end

if k.bypassChargeSpeed then
local aw=as:FindFirstChild"Guest1337Charge"
if aw then
aw.Value=3.4
end
end

if k.bypassPunchSpeed then
local aw=as:FindFirstChild"PunchAbility"
if aw then
aw.Value=1.8
end
end

if k.antiTaphTripwire then
local aw=as:FindFirstChild"TaphTripwire"
if aw then
aw.Value=1.8
end
end

if k.antiSubspaceTripmine then
local aw=as:FindFirstChild"SubspaceTripmine"
if aw then
aw.Value=1.8
end
end

if k.antiPlasmaBeam then
local aw=as:FindFirstChild"PlasmaBeam"
if aw then
aw.Value=1.8
end
end

if k.antiSpawnProtection then
local aw=as:FindFirstChild"SpawnProtection"
if aw then
aw.Value=1.8
end
end

if k.antiTwoTimeCrouching then
local aw=as:FindFirstChild"TwoTimeCrouching"
if aw then
aw.Value=1.8
end
end

if k.antiTwoTimeStab then
local aw=as:FindFirstChild"TwoTimeStab"
if aw then
aw.Value=1.8
end
end

if k.antiCoolGui then
local aw=as:FindFirstChild"c00lgui"
if aw then
aw.Value=1.8
end
end

if k.antiBehead then
local aw=as:FindFirstChild"BeheadAbility"
if aw then
aw.Value=1.8
end
end

if k.antiEnraged then
local aw=as:FindFirstChild"ENRAGED"
if aw then
aw.Value=2.7
end
end

if k.antiShootDebuff then
local aw=as:FindFirstChild"ShootingGun"
if aw then
aw.Value=1.8
end
end
end

local at=ar:FindFirstChild"FOVMultipliers"
if at then
local au=at:FindFirstChild"SlowedStatus"
if au and(k.autoRemovePopups or k.antiSlowAfterChargeEnd or k.antiSpawnProtection)then
au:Destroy()
end

if k.antiSlateskinDebuff then
local av=at:FindFirstChild"SlateskinStatus"
if av then
av:Destroy()
end
end

if k.antiAutoFovCharge then
local av=at:FindFirstChild"Guest1337Charge"
if av then
av:Destroy()
end
end
end
end
end
end

if k.autoRemovePopups then
for ao,ap in ipairs(a:GetPlayers())do
local aq=ap:FindFirstChild"PlayerGui"
if aq then
local ar=aq:FindFirstChild"TemporaryUI"
if ar then
local as=ar:FindFirstChild"1x1x1x1Popup"
if as then
as:Destroy()
end
end
end
end
end

if k.antiMedkitDebuff then
local ao,ap=pcall(function()
return require(c.Assets.Items.Medkit.Config)
end)
if ao and ap then
ap.HealTime=0
ap.HealSpeedMultiplier=1
end
end

task.wait(0.05)
else
task.wait(0.1)
end
end
end)


if not _G.FullBrightExecuted then
_G.FullBrightEnabled=false
_G.NormalLightingSettings={
Brightness=game:GetService"Lighting".Brightness,
ClockTime=game:GetService"Lighting".ClockTime,
FogEnd=game:GetService"Lighting".FogEnd,
GlobalShadows=game:GetService"Lighting".GlobalShadows,
Ambient=game:GetService"Lighting".Ambient
}

local function removeEffects()
local an=game:GetService"Lighting"
for ao,ap in ipairs{"DepthOfField","ColorCorrection","Blur","Atmosphere"}do
local aq=an:FindFirstChild(ap)
if aq then
aq:Destroy()
end
end
end

local an=game:GetService"Lighting"
an.Changed:Connect(function(ao)
if _G.FullBrightEnabled then
if ao=="Brightness"then
an.Brightness=1
elseif ao=="ClockTime"then
an.ClockTime=12
elseif ao=="FogEnd"then
an.FogEnd=786543
elseif ao=="GlobalShadows"then
an.GlobalShadows=false
elseif ao=="Ambient"then
an.Ambient=Color3.fromRGB(178,178,178)
end
removeEffects()
else
if ao=="Brightness"then
_G.NormalLightingSettings.Brightness=an.Brightness
elseif ao=="ClockTime"then
_G.NormalLightingSettings.ClockTime=an.ClockTime
elseif ao=="FogEnd"then
_G.NormalLightingSettings.FogEnd=an.FogEnd
elseif ao=="GlobalShadows"then
_G.NormalLightingSettings.GlobalShadows=an.GlobalShadows
elseif ao=="Ambient"then
_G.NormalLightingSettings.Ambient=an.Ambient
end
end
end)

task.spawn(function()
while true do
if _G.FullBrightEnabled then
removeEffects()
end
task.wait(0.1)
end
end)

local function applyFullBright()
if _G.FullBrightEnabled then
an.Brightness=1
an.ClockTime=12
an.FogEnd=786543
an.GlobalShadows=false
an.Ambient=Color3.fromRGB(178,178,178)
removeEffects()
else
an.Brightness=_G.NormalLightingSettings.Brightness
an.ClockTime=_G.NormalLightingSettings.ClockTime
an.FogEnd=_G.NormalLightingSettings.FogEnd
an.GlobalShadows=_G.NormalLightingSettings.GlobalShadows
an.Ambient=_G.NormalLightingSettings.Ambient
end
end

local ao=false
spawn(function()
while wait()do
if _G.FullBrightEnabled~=ao then
applyFullBright()
ao=_G.FullBrightEnabled
end
end
end)

_G.FullBrightExecuted=true
end


task.spawn(function()
while true do
if isGameInProgress()then
updateAllESP()
end
task.wait(0.1)
end
end)


local function initialSetup()
if isGameInProgress()then
updateAllESP()
end
end

initialSetup()


b.Heartbeat:Connect(function()
if k.autoCoinFlip and isGameInProgress()then
local an=h.PlayerGui:FindFirstChild"MainUI"and
h.PlayerGui.MainUI:FindFirstChild"AbilityContainer"and
h.PlayerGui.MainUI.AbilityContainer:FindFirstChild"Shoot"and
h.PlayerGui.MainUI.AbilityContainer.Shoot:FindFirstChild"Charges"

local ao=h.PlayerGui:FindFirstChild"MainUI"and
h.PlayerGui.MainUI:FindFirstChild"AbilityContainer"and
h.PlayerGui.MainUI.AbilityContainer:FindFirstChild"Reroll"and
h.PlayerGui.MainUI.AbilityContainer.Reroll:FindFirstChild"Charges"

local ap=h.PlayerGui:FindFirstChild"MainUI"and
h.PlayerGui.MainUI:FindFirstChild"AbilityContainer"and
h.PlayerGui.MainUI.AbilityContainer:FindFirstChild"HatFix"and
h.PlayerGui.MainUI.AbilityContainer.HatFix:FindFirstChild"Charges"

local aq=an and an:IsA"TextLabel"and an.Text
local ar=ao and ao:IsA"TextLabel"and ao.Text
local as=ap and ap:IsA"TextLabel"and ap.Text

local at=true

if aq and aq=="3"then
at=false
end
if ar and ar=="3"then
at=false
end
if as and as=="1"then
at=false
end

if at then
local au=game:GetService"ReplicatedStorage".Modules.Network.RemoteEvent
au:FireServer("UseActorAbility","CoinFlip")
end
task.wait(0.5)
end
end)


b.RenderStepped:Connect(function(an)
if k.cframeSpeedEnabled and h.Character and h.Character:FindFirstChild"HumanoidRootPart"then
local ao=h.Character.HumanoidRootPart
local ap=h.Character.Humanoid.MoveDirection
if ap.Magnitude>0 then
ao.CFrame=ao.CFrame+ap*k.cframeSpeedValue*an
end
end
end)


local an=0
b.RenderStepped:Connect(function(ao)
if k.aimbotRGB then
an=(an+ao*90)%360
local ap=Color3.fromHSV(an/360,1,1)
n.Color=ap
else
n.Color=Color3.new(1,1,1)
end

if k.aimbotFOVVisible then
m.Visible=true
local ap=k.aimbotFOV
m.Size=UDim2.new(0,ap*2,0,ap*2)
m.Position=UDim2.new(0.5,-ap,0.5,-ap)
else
m.Visible=false
end
end)
