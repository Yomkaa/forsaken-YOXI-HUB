local function collectItem(y)
local z=h.Character
if not z or not z:FindFirstChild"HumanoidRootPart"then
return false
end

local A=y:FindFirstChild"ItemRoot"
if A and A:FindFirstChild"ProximityPrompt"then
local B=A.ProximityPrompt
z.HumanoidRootPart.CFrame=A.CFrame
task.wait(0.5)
fireproximityprompt(B)
task.wait(1)
return true
end
return false
end

local function createESP(y,z,A,B,C)
if y:FindFirstChild"TAOWARE_Highlight"then return end
if A and y:FindFirstChild"Progress"and y.Progress.Value==100 then return end

local D
if A then
D=y:FindFirstChild"Main"
if not D then return end
elseif B then
D=y:FindFirstChild"ItemRoot"
if not D then return end
elseif C then
D=y
if not D then return end
else
D=y:FindFirstChild"HumanoidRootPart"
if not D then return end
end

local E=Instance.new"Highlight"
E.Name="TAOWARE_Highlight"
E.Adornee=y
E.FillColor=z
E.OutlineColor=z
E.FillTransparency=0.5
E.OutlineTransparency=0
E.Parent=y

local F=Instance.new"BillboardGui"
F.Name="TAOWARE_Billboard"
F.Adornee=D
F.Size=UDim2.new(0,150,0,60)
F.StudsOffset=Vector3.new(0,4,0)
F.AlwaysOnTop=true
F.Parent=y

if not A and not B and not C then
local G=a:GetPlayerFromCharacter(y)
local H=G and G.Name or"Unknown"
local I=y:FindFirstChild"Humanoid"
local J=I and string.format("%.0f",I.Health)or"N/A"
local K="Unknown"
if G and G:FindFirstChild"PlayerData"and G.PlayerData:FindFirstChild"Equipped"then
if y.Parent.Name=="Killers"and G.PlayerData.Equipped:FindFirstChild"Killer"then
K=G.PlayerData.Equipped.Killer.Value
elseif y.Parent.Name=="Survivors"and G.PlayerData.Equipped:FindFirstChild"Survivor"then
K=G.PlayerData.Equipped.Survivor.Value
end
end

local L=Instance.new"TextLabel"
L.Size=UDim2.new(1,0,0.33,0)
L.Position=UDim2.new(0,0,0,0)
L.BackgroundTransparency=1
L.Text=H
L.Font=Enum.Font.GothamBlack
L.TextColor3=z
L.TextSize=12
L.TextStrokeTransparency=0.6
L.TextStrokeColor3=Color3.fromRGB(0,0,0)
L.Parent=F

local M=Instance.new"TextLabel"
M.Size=UDim2.new(1,0,0.33,0)
M.Position=UDim2.new(0,0,0.33,0)
M.BackgroundTransparency=1
M.Text="HP: "..J
M.Font=Enum.Font.GothamBlack
M.TextColor3=z
M.TextSize=12
M.TextStrokeTransparency=0.6
M.TextStrokeColor3=Color3.fromRGB(0,0,0)
M.Parent=F

local N=Instance.new"TextLabel"
N.Size=UDim2.new(1,0,0.33,0)
N.Position=UDim2.new(0,0,0.66,0)
N.BackgroundTransparency=1
N.Text=K
N.Font=Enum.Font.GothamBlack
N.TextColor3=z
N.TextSize=12
N.TextStrokeTransparency=0.6
N.TextStrokeColor3=Color3.fromRGB(0,0,0)
N.Parent=F
else
local G=Instance.new"TextLabel"
G.Size=UDim2.new(1,0,1,0)
G.BackgroundTransparency=1
G.Text=y.Name
G.Font=Enum.Font.GothamBlack
G.TextColor3=z
G.TextSize=A and 14 or 12
G.TextStrokeTransparency=0.6
G.TextStrokeColor3=Color3.fromRGB(0,0,0)
G.Parent=F
end
end

local function removeESP(y)
if y:FindFirstChild"TAOWARE_Highlight"then
y.TAOWARE_Highlight:Destroy()
end
if y:FindFirstChild"TAOWARE_Billboard"then
y.TAOWARE_Billboard:Destroy()
end
end

local function getClosestTarget(y)
local z=d:FindFirstChild"Players"
if not z then return nil end

local A
if y=="killer"then
local B=z:FindFirstChild"Killers"
if B then
A=B:GetChildren()
else
A={}
end
elseif y=="survivor"then
local B=z:FindFirstChild"Survivors"
if B then
A=B:GetChildren()
else
A={}
end
else
return nil
end

local B=nil
local C=k.aimbotFOV

for D,E in ipairs(A)do
if E==h.Character then continue end
local F=E:FindFirstChildOfClass"Humanoid"
if not F or F.Health<=0 then continue end
local G=E:FindFirstChild"HumanoidRootPart"
if not G then continue end

local H,I=i:WorldToViewportPoint(G.Position)
if not I then continue end

local J=(Vector2.new(H.X,H.Y)-Vector2.new(i.ViewportSize.X/2,i.ViewportSize.Y/2)).Magnitude
if J>k.aimbotFOV then continue end

if k.aimbotWallCheck then
local K=i.CFrame.Position
local L=G.Position-K
local M=RaycastParams.new()
M.FilterDescendantsInstances={h.Character,E}
M.FilterType=Enum.RaycastFilterType.Blacklist
local N=d:Raycast(K,L,M)
if N then continue end
end

if J<C then
C=J
B=E
end
end

return B
end


local function isGameInProgress()
local y=d:FindFirstChild"Map"
if not y or not y:FindFirstChild"Ingame"then
return false
end

local z=d:FindFirstChild"Players"
if z then
local A=z:FindFirstChild"Survivors"
if A and#A:GetChildren()>0 then
return true
end
end

return false
end


local function hasGenerators()
local y=d:FindFirstChild"Map"
if y and y:FindFirstChild"Ingame"and y.Ingame:FindFirstChild"Map"then
for z,A in ipairs(y.Ingame.Map:GetChildren())do
if A:IsA"Model"and A.Name:lower():find"generator"then
return true
end
end
end
return false
end


local y=false

local function runAutoGenerator()
if y then return end
y=true
task.spawn(function()
while y and k.autoGenerator and hasGenerators()and isGameInProgress()do
pcall(function()
local z=d:FindFirstChild"Map"
if not z then return end
local A=z:FindFirstChild"Ingame"
if not A then return end
local B=A:FindFirstChild"Map"
if not B then return end

for C,D in ipairs(B:GetChildren())do
if D:IsA"Model"and D.Name:lower():find"generator"then
local E=D:FindFirstChild"Remotes"
if E then
local F=E:FindFirstChild"RE"
local G=E:FindFirstChild"RF"
if F and G then
pcall(function()F:FireServer()end)
pcall(function()G:InvokeServer()end)
end
end
task.wait(0.1)
end
end
end)
task.wait(k.generatorWaitTime)
end
y=false
end)
end

local function restartAutoGenerator()
y=false
task.wait(0.1)
if k.autoGenerator then
runAutoGenerator()
end
end


