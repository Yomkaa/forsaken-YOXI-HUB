local function handleKeyPress(H)
local I=tick()
local J=tostring(H)

if A[J]and(I-A[J])<B then
return
end
A[J]=I

if H==Enum.KeyCode.X then
restartAutoGenerator()
F.new({
Title="Auto Generator",
Description="Auto Generator Restarted!",
Duration=3,
Icon="rbxassetid://82898985676372"
})
elseif H==Enum.KeyCode.V then
p.autoCollectItems=not p.autoCollectItems
F.new({
Title="Auto Collect",
Description="Auto Collect Items: "..(p.autoCollectItems and"ON"or"OFF"),
Duration=3,
Icon="rbxassetid://82898985676372"
})
elseif H==Enum.KeyCode.N then
p.noClip=not p.noClip
if m.Character then
if p.noClip then
w={}
for K,L in ipairs(m.Character:GetDescendants())do
if L:IsA("BasePart")then
w[L]=L.CanCollide
L.CanCollide=false
end
end
u=false
if v then
v:Disconnect()
end
v=b.Stepped:Connect(function()
if not u and m.Character then
for K,L in ipairs(m.Character:GetDescendants())do
if L:IsA("BasePart")and L.Parent then
L.CanCollide=false
end
end
end
end)
F.new({
Title="NoClip",
Description="NoClip Enabled!",
Duration=3,
Icon="rbxassetid://82898985676372"
})
else
u=true
if v then
v:Disconnect()
v=nil
end
for K,L in pairs(w)do
if K and K.Parent then
K.CanCollide=L
end
end
w={}
F.new({
Title="NoClip",
Description="NoClip Disabled!",
Duration=3,
Icon="rbxassetid://82898985676372"
})
end
end
end
end

f.InputBegan:Connect(function(H,I)
if I then return end

if H.UserInputType==Enum.UserInputType.Keyboard then
handleKeyPress(H.KeyCode)
end
end)


