local function handleKeyPress(C)
local D=tick()
local E=tostring(C)

if v[E]and(D-v[E])<w then
return
end
v[E]=D

if C==Enum.KeyCode.X then
restartAutoGenerator()
A.new{
Title="Auto Generator",
Description="Auto Generator Restarted!",
Duration=3,
Icon="rbxassetid://82898985676372"
}
elseif C==Enum.KeyCode.V then
k.autoCollectItems=not k.autoCollectItems
A.new{
Title="Auto Collect",
Description="Auto Collect Items: "..(k.autoCollectItems and"ON"or"OFF"),
Duration=3,
Icon="rbxassetid://82898985676372"
}
elseif C==Enum.KeyCode.N then
k.noClip=not k.noClip
if h.Character then
if k.noClip then
r={}
for F,G in ipairs(h.Character:GetDescendants())do
if G:IsA"BasePart"then
r[G]=G.CanCollide
G.CanCollide=false
end
end
p=false
if q then
q:Disconnect()
end
q=b.Stepped:Connect(function()
if not p and h.Character then
for F,G in ipairs(h.Character:GetDescendants())do
if G:IsA"BasePart"and G.Parent then
G.CanCollide=false
end
end
end
end)
A.new{
Title="NoClip",
Description="NoClip Enabled!",
Duration=3,
Icon="rbxassetid://82898985676372"
}
else
p=true
if q then
q:Disconnect()
q=nil
end
for F,G in pairs(r)do
if F and F.Parent then
F.CanCollide=G
end
end
r={}
A.new{
Title="NoClip",
Description="NoClip Disabled!",
Duration=3,
Icon="rbxassetid://82898985676372"
}
end
end
end
end

f.InputBegan:Connect(function(C,D)
if D then return end

if C.UserInputType==Enum.UserInputType.Keyboard then
handleKeyPress(C.KeyCode)
end
end)


